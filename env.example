[config]
DB_TYPE="sqlanywhere"
MYSQL_DRIVER="mysql"
MYSQL_HOST="127.0.0.1"
MYSQL_PORT="3306"
MYSQL_DATABASE="dcrphp"
MYSQL_USERNAME="dcrphp"
MYSQL_PASSWORD="dcrphp"
MYSQL_CHARSET="utf8"
ADMIN_RESOURCE_URL="/resource/houtai/"
APP_RESOURCE_URL="/resource/help/"

CACHE_ENABLE=0
CACHE_DRIVER=file
FILE_CACHE_PATH=storage/cache

LOG_ENABLE=0
LOG_CHANNEL=dcrphp
LOG_DRIVER=directory
LOG_DRIVER_ERROR=directory
FILE_LOG_PATH=storage/log.log
DIRECTORY_LOG_PATH=storage/log
DIRECTORY_LOG_PREFIX=php
DIRECTORY_LOG_GENERAL=hour